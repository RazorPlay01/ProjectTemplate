{
  "schemaVersion": 1,
  "id": "${project_id}",
  "version": "${version}",
  "name": "${project_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "${web}"
  },
  "license": "${license}",
  "icon": "assets/${project_id}/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.github.razorplay01.template.FabricTemplate"
    ],
    "main": [
      "com.github.razorplay01.template.FabricTemplate"
    ]
  },
  "mixins": [
    "${project_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
